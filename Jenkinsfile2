
pipeline {
    agent any
    tools {nodejs "nodejs"}
    
    stages{
       
       stage ("cloning") {
            steps{
                echo "cloning"
               sh "git clone https://github.com/contentful/the-example-app.nodejs.git"
                 }
            }
       
       stage ("build") {
        steps {
        sh ' docker build -t mona88/nodeapp_test:latest . '
              }
              }
        }
       
}
